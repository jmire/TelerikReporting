                                                            </Border>
                                                        </ControlTemplate>
                                                        <ControlTemplate x:Key="InteractiveTemplate">
                                                            <ContentPresenter Margin="10" 
                                                                                          HorizontalAlignment="Left"
                                                                                          VerticalAlignment="Top" />
                                                        </ControlTemplate>
