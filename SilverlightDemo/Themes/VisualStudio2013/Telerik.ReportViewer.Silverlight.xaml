                                                        <telerikControls:RadButton Command="{Binding CancelExportCommand}"
                                                                                   Grid.Column="2"
                                                                                   Padding="10, 3"                                         
                                                                                   Margin="5"                                                                               
                                                                                   Content="{Binding TextResources.CancelExportButtonText}"
                                                                                   />
