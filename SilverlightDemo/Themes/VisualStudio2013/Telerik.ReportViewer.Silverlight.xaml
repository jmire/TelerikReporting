<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewer="clr-namespace:Telerik.ReportViewer.Silverlight;assembly=Telerik.ReportViewer.Silverlight"
    xmlns:winControls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls" 
    xmlns:telerikNav="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Navigation"
    xmlns:telerikControls="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls"
    xmlns:telerikPrimitives ="clr-namespace:Telerik.Windows.Controls.Primitives;assembly=Telerik.Windows.Controls"
    xmlns:telerikInput="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.Input">

    <!-- VisualStudio2013Theme -->
    <viewer:VisibilityConverter x:Key="VisibilityConverter" />
    <viewer:ZoomConverter x:Key="ZoomConverter" />
    <viewer:ScaleFactorConverter x:Key="ScaleFactorConverter" />
    <viewer:MultivalueParameterConverter x:Key="MultivalueParameterConverter" />

    <Style TargetType="viewer:DialogBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="viewer:DialogBox">
                    <Border>
                        <Border.Background>
                            <SolidColorBrush Color="Gray" Opacity=".3" />
                        </Border.Background>
                        <Border Background="{telerikControls:VisualStudio2013Resource ResourceKey=MainBrush}" 
                                Margin="20"
                                VerticalAlignment="Center" 
                                HorizontalAlignment="Center">
                            <Border.Effect>
                                <DropShadowEffect />
                            </Border.Effect>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="viewer:ErrorStatus">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="viewer:ErrorStatus">
                    <Grid x:Name="LayoutRoot" >
                        <Ellipse Fill="{telerikControls:VisualStudio2013Resource ResourceKey=ValidationBrush}"/>
                        <TextBlock Foreground="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" 
                                   HorizontalAlignment="Center" 
                                   VerticalAlignment="Stretch" 
                                   FontWeight="Bold" >!</TextBlock>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <telerikControls:ContainerBindingCollection  x:Name="DocumentMapItemBindingsCollection">
        <telerikControls:ContainerBinding PropertyName="IsExpanded" Binding="{Binding IsExpanded, Mode=TwoWay}" />
    </telerikControls:ContainerBindingCollection>

    <Style TargetType="ContentControl" x:Key="ParameterEditorStyle">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Grid Margin="5" Height="30">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" MinWidth="100" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="auto" />
                        </Grid.ColumnDefinitions>

                        <TextBlock Grid.Column="0" 
                                                           TextWrapping="Wrap" 
                                                           Foreground="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}"
                                                           Text="{Binding Text}" 
                                                           VerticalAlignment="Center" 
                                                           HorizontalAlignment="Left" />
                        <ContentPresenter Grid.Column="1" Margin="5, 0" VerticalAlignment="Center" />
                        <CheckBox Grid.Column="2" 
                                                          VerticalAlignment="Center"
                                                          Foreground="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}"
                                                          Content="{Binding TextResources.ReportParametersNullText}"
                                                          IsChecked="{Binding IsNull, Mode=TwoWay}"                                   
                                                          Visibility="{Binding AllowNull, Converter={StaticResource VisibilityConverter}}" />
                        <viewer:ErrorStatus Grid.Column="3" IsTabStop="False" PropertyName="Error" Width="15" Height="15" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <viewer:ReportParameterEditorTemplateSelector x:Key="ReportParameterEditorTemplateSelector">
        <viewer:ReportParameterEditorTemplateSelector.Templates>
            <!-- 
                                                Note:
                                                Order the templates from most specific to most general (less specific).
                                                The 1st template that matches the parameter is picked 
                                            -->

            <!-- BOOLEAN -->
            <viewer:ReportParameterTemplate Type="System.Boolean">
                <ContentControl Style="{StaticResource ParameterEditorStyle}">
                    <CheckBox IsChecked="{Binding Value, Mode=TwoWay}" IsEnabled="{Binding IsEnabled}"/>
                </ContentControl>
            </viewer:ReportParameterTemplate>

            <!-- MULTIVALUE + AVAILABLE VALUES -->
            <viewer:ReportParameterTemplate AvailableValues="true" Multivalue="True">
                <ContentControl Style="{StaticResource ParameterEditorStyle}">
                    <telerikInput:RadComboBox Width="200" 
                                              ItemsSource="{Binding AvailableValues}" 
                                              SelectedIndex="0"
                                              IsDropDownOpen="{Binding IsDropDownOpen, Mode=TwoWay}"
                                              IsEnabled="{Binding IsEnabled}">
                        <telerikInput:RadComboBox.ItemTemplate>
                            <DataTemplate>
                                <CheckBox IsChecked="{Binding IsSelected, Mode=TwoWay}" Content="{Binding Name}" />
                            </DataTemplate>
                        </telerikInput:RadComboBox.ItemTemplate>
                        <telerikInput:RadComboBox.SelectionBoxTemplate>
                            <DataTemplate>
                                <TextBlock HorizontalAlignment="Stretch" 
                                           Text="{Binding Owner.SelectedLabels, Mode=OneWay, Converter={StaticResource MultivalueParameterConverter}}" />
                            </DataTemplate>
                        </telerikInput:RadComboBox.SelectionBoxTemplate>
                    </telerikInput:RadComboBox>
                </ContentControl>
            </viewer:ReportParameterTemplate>

            <!-- MULTIVALUE + NO AVAILABLE VALUES -->
            <viewer:ReportParameterTemplate Multivalue="True" AvailableValues="False">
                <ContentControl Style="{StaticResource ParameterEditorStyle}">
                    <TextBox Text="{Binding Value, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true, Converter={StaticResource MultivalueParameterConverter}}"
                             Width="200" IsEnabled="{Binding IsEnabled}" />
                </ContentControl>
            </viewer:ReportParameterTemplate>

            <!-- AVAILABLE VALUES -->
            <viewer:ReportParameterTemplate AvailableValues="true">
                <ContentControl Style="{StaticResource ParameterEditorStyle}">
                    <telerikInput:RadComboBox Width="200" IsEnabled="{Binding IsEnabled}"
                                                                              ItemsSource="{Binding AvailableValues}" 
                                                                              SelectedItem="{Binding SelectedSingleValue, Mode=TwoWay}">
                        <telerikInput:RadComboBox.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Name}" />
                            </DataTemplate>
                        </telerikInput:RadComboBox.ItemTemplate>
                    </telerikInput:RadComboBox>
                </ContentControl>
            </viewer:ReportParameterTemplate>

            <!-- DATETIME -->
            <viewer:ReportParameterTemplate Type="System.DateTime">
                <ContentControl Style="{StaticResource ParameterEditorStyle}">
                    <telerikInput:RadDatePicker Width="200" 
                                                IsEnabled="{Binding IsEnabled}"
                                                SelectedDate="{Binding Value, Mode=TwoWay}" />
                </ContentControl>
            </viewer:ReportParameterTemplate>

            <!-- GENERIC EDITOR -->
            <viewer:ReportParameterTemplate Multivalue="False">
                <ContentControl Style="{StaticResource ParameterEditorStyle}">
                    <TextBox Text="{Binding Value, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}"
                             Width="200" 
                             IsEnabled="{Binding IsEnabled}"/>
                </ContentControl>
            </viewer:ReportParameterTemplate>

        </viewer:ReportParameterEditorTemplateSelector.Templates>
    </viewer:ReportParameterEditorTemplateSelector>

    <Style x:Key="ExportDialogStyle" TargetType="viewer:DialogBox">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid Width="270">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="100" />
                            <RowDefinition Height="auto" />
                        </Grid.RowDefinitions>

                        <Grid Margin="20" VerticalAlignment="Center" HorizontalAlignment="Stretch">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>

                            <TextBlock Text="{Binding Status}" VerticalAlignment="Center" HorizontalAlignment="Center"
                                       Foreground="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}"/>

                            <telerikControls:RadProgressBar Grid.Row="1"
                                                            Visibility="Collapsed"
                                                            IsIndeterminate="True"
                                                            Height="10"
                                                            Margin=" 0 10 0 0"
                                                            IsTabStop="False" />
                        </Grid>
                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>

                            <Button x:Name="SaveExport"
                                                                    Grid.Column="1"
                                                                    Padding="10, 3"
                                                                    Margin="5"
                                                                    IsEnabled="{Binding DocumentReady}"
                                                                    Content="{Binding TextResources.SaveButtonText}" />
                            <HyperlinkButton Grid.Column="2"
                                                                             Padding="10, 3" 
                                                                             Margin="5"
                                                                             x:Name="CancelExport"
                                                                             Content="{Binding TextResources.CancelExportButtonText}" />
                        </Grid>
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="viewer:ReportViewer">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="viewer:ReportViewer">
                    <Grid x:Name="LayoutRoot" DataContext="{x:Null}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="ExportReportStateGroup">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition GeneratedDuration="0" />
                                </VisualStateGroup.Transitions>
                                <VisualState x:Name="ExportNoExportState" />
                                <VisualState x:Name="ExportInProgressState">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ExportDialog"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ExportProgressBar"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ExportCompleteState">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ExportDialog"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="PrintReportStateGroup">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition GeneratedDuration="0" />
                                </VisualStateGroup.Transitions>
                                <VisualState x:Name="PrintNoPrintState" />
                                <VisualState x:Name="PrintInProgressState">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PrintDialog"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PrintProgressBar"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="PrintCompleteState">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PrintDialog"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="ViewerStateGroup">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition GeneratedDuration="0" />
                                </VisualStateGroup.Transitions>
                                <VisualState x:Name="ViewerPageState"/>
                                <VisualState x:Name="ViewerErrorState">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ViewerError"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PageScrollViewer"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Collapsed</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="ViewerWorkInProgressState">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ViewerProgressMessage"
                                                                       Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <Border Padding="6"
                                Background="{telerikControls:VisualStudio2013Resource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerikControls:VisualStudio2013Resource ResourceKey=BasicBrush}"
                                BorderThickness="1">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="6" />
                                    <RowDefinition Height="auto" />
                                </Grid.RowDefinitions>

                                <!-- Toolbar -->
                                <telerikNav:RadToolBar Grid.Row="0" Margin="0">
                                    <telerikControls:RadButton x:Name="Nav_Back" Command="{Binding NavigateBackCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16" VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Ellipse Stroke="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" StrokeThickness="1.7"/>
                                                <Path Data="M3,8 7,4 7,7 13,7 13,9 7,9 7,12 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=Nav_Back}" >
                                                <Ellipse Stroke="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" StrokeThickness="1.7"/>
                                                <Path Data="M3,8 7,4 7,7 13,7 13,9 7,9 7,12 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsPressed, Converter={StaticResource VisibilityConverter},ElementName=Nav_Back}" >
                                                <Ellipse Stroke="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" StrokeThickness="1.7"/>
                                                <Path Data="M3,8 7,4 7,7 13,7 13,9 7,9 7,12 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" />
                                            </Grid>
                                        </Grid>
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.BackToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </telerikControls:RadButton>

                                    <telerikControls:RadButton x:Name="Nav_Forward" Command="{Binding NavigateForwardCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16"  VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Ellipse Stroke="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" StrokeThickness="1.7"/>
                                                <Path Data="M3,7 9,7 9,4 13,8 9,12 9,9 3,9 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=Nav_Forward}" >
                                                <Ellipse Stroke="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" StrokeThickness="1.7"/>
                                                <Path Data="M3,7 9,7 9,4 13,8 9,12 9,9 3,9 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsPressed, Converter={StaticResource VisibilityConverter},ElementName=Nav_Forward}" >
                                                <Ellipse Stroke="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" StrokeThickness="1.7"/>
                                                <Path Data="M3,7 9,7 9,4 13,8 9,12 9,9 3,9 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" />
                                            </Grid>
                                        </Grid>
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.ForwardToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </telerikControls:RadButton>

                                    <telerikNav:RadToolBarSeparator />

                                    <telerikControls:RadButton x:Name="Refresh" Command="{Binding RefreshReportCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16"  VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Path Data="M13.8,9 L14,9 L16,9 C15.4,13 12,16 8,16 C5.8,16 3.9,15 2.5,13.7 L2.4,13.6 L0,16 L0,10 L2.4,10 L6,10 L3.8,12.2 L3.8,12.3 C4.9,13.3 6.4,14 8,14 C11,14 13.3,11.9 13.8,9 z M8,0 C10,0 12,0.8 13.5,2.3 L13.6,2.4 L16,0 L16,6 L13.5,6 L10,6 L12.2,3.8 L12.1,3.7 C11,2.6 9.6,2 8,2 C4.7,2 2,4.7 2,8 L0,8 C0,3.6 3.6,0 8,0 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=Refresh}" >
                                                <Path Data="M13.8,9 L14,9 L16,9 C15.4,13 12,16 8,16 C5.8,16 3.9,15 2.5,13.7 L2.4,13.6 L0,16 L0,10 L2.4,10 L6,10 L3.8,12.2 L3.8,12.3 C4.9,13.3 6.4,14 8,14 C11,14 13.3,11.9 13.8,9 z M8,0 C10,0 12,0.8 13.5,2.3 L13.6,2.4 L16,0 L16,6 L13.5,6 L10,6 L12.2,3.8 L12.1,3.7 C11,2.6 9.6,2 8,2 C4.7,2 2,4.7 2,8 L0,8 C0,3.6 3.6,0 8,0 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsPressed, Converter={StaticResource VisibilityConverter},ElementName=Refresh}" >
                                                <Path Data="M13.8,9 L14,9 L16,9 C15.4,13 12,16 8,16 C5.8,16 3.9,15 2.5,13.7 L2.4,13.6 L0,16 L0,10 L2.4,10 L6,10 L3.8,12.2 L3.8,12.3 C4.9,13.3 6.4,14 8,14 C11,14 13.3,11.9 13.8,9 z M8,0 C10,0 12,0.8 13.5,2.3 L13.6,2.4 L16,0 L16,6 L13.5,6 L10,6 L12.2,3.8 L12.1,3.7 C11,2.6 9.6,2 8,2 C4.7,2 2,4.7 2,8 L0,8 C0,3.6 3.6,0 8,0 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" />
                                            </Grid>
                                        </Grid>
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.RefreshToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </telerikControls:RadButton>

                                    <telerikNav:RadToolBarSeparator />

                                    <telerikControls:RadButton x:Name="First_Page" Command="{Binding MoveToFirstPageCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16"  VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Path Data="M2,1 4,1 4,15 2,15 z M14,1 14,15 5,8 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=First_Page}" >
                                                <Path Data="M2,1 4,1 4,15 2,15 z M14,1 14,15 5,8 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsPressed, Converter={StaticResource VisibilityConverter},ElementName=First_Page}" >
                                                <Path Data="M2,1 4,1 4,15 2,15 z M14,1 14,15 5,8 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" />
                                            </Grid>
                                        </Grid>
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.FirstPageToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </telerikControls:RadButton>

                                    <telerikControls:RadButton x:Name="Prev_Page" Command="{Binding MoveToPreviousPageCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16"  VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Path Data="M12,1 12,15 2,8 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=Prev_Page}" >
                                                <Path Data="M12,1 12,15 2,8 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsPressed, Converter={StaticResource VisibilityConverter},ElementName=Prev_Page}" >
                                                <Path Data="M12,1 12,15 2,8 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" />
                                            </Grid>
                                        </Grid>
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.PreviousPageToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </telerikControls:RadButton>

                                    <telerikNav:RadToolBarSeparator />
                                    <viewer:TextBoxEx Text="{Binding PageNumberModel, Mode=TwoWay}" 
                                                          IsEnabled="{Binding IsMoveToPageEnabled}" 
                                                          MinWidth="30" 
                                                          Margin="10, 0, 0, 0" 
                                                          VerticalAlignment="Center" 
                                                          TextAlignment="Center"
                                                          viewer:KeyDown.Command="{Binding NavigateToSpecificPageCommand}">
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.CurrentPageToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </viewer:TextBoxEx>

                                    <TextBlock Text="{Binding TextResources.LabelOf}" 
                                                       Margin="2, 0, 2, 0"  
                                                       VerticalAlignment="Center" 
                                                       TextAlignment="Center">
                                            <ToolTipService.ToolTip>
                                                <ToolTip Content="{Binding TextResources.TotalPagesToolTip}"/>
                                            </ToolTipService.ToolTip>
                                    </TextBlock>
                                    <TextBlock Text="{Binding PageCount}" 
                                                   Margin="2, 0, 10, 0" 
                                                   VerticalAlignment="Center" 
                                                   TextAlignment="Center">
                                             <ToolTipService.ToolTip>
                                                <ToolTip Content="{Binding TextResources.TotalPagesToolTip}"/>
                                            </ToolTipService.ToolTip>
                                    </TextBlock>
                                    <telerikNav:RadToolBarSeparator />

                                    <telerikControls:RadButton x:Name="Next_Page" Command="{Binding MoveToNextPageCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16"  VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Path Data="M2,1 12,8 2,15 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=Next_Page}" >
                                                <Path Data="M2,1 12,8 2,15 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsPressed, Converter={StaticResource VisibilityConverter},ElementName=Next_Page}" >
                                                <Path Data="M2,1 12,8 2,15 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=SelectedBrush}" />
                                            </Grid>
                                        </Grid>
                                        <ToolTipService.ToolTip>
                                            <ToolTip Content="{Binding TextResources.NextPageToolTip}"/>
                                        </ToolTipService.ToolTip>
                                    </telerikControls:RadButton>

                                    <telerikControls:RadButton x:Name="Last_Page" Command="{Binding MoveToLastPageCommand}" Margin="0,2" Padding="2">
                                        <Grid HorizontalAlignment="Center" Height="16"  VerticalAlignment="Top" Width="16">
                                            <Grid>
                                                <Path Data="M1,1 11,8 1,15 z M12,1 14,1 14,15 12,15 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=StrongBrush}" />
                                            </Grid>
                                            <Grid Visibility="{Binding IsMouseOver, Converter={StaticResource VisibilityConverter},ElementName=Last_Page}" >
                                                <Path Data="M1,1 11,8 1,15 z M12,1 14,1 14,15 12,15 z" Fill="{telerikControls:VisualStudio2013Resource ResourceKey=MarkerBrush}" />
                                            </Grid>
