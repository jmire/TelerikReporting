                                                                                   Grid.Column="1"                                                           
                                                                                   Padding="10, 3"
                                                                                   Margin="5"
                                                                                   Content="{Binding TextResources.PrintButtonText}" 
                                                                                   />

                                                        <telerikControls:RadButton Command="{Binding CancelPrintDocumentCommand}"
                                                                                   Grid.Column="2"
                                                                                   Padding="10, 3"                                         
                                                                                   Margin="5"                                                                               
                                                                                   Content="{Binding TextResources.CancelPrintButtonText}"
                                                                                   />
                                                    </Grid>
                                                </Grid>
                                            </viewer:DialogBox>
                                        </Grid>
                                    </Border>
                                </Grid>

                                <!-- Status Area -->
                                <Grid  Grid.Row="5">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="auto"/>
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Grid.Column="1" 
                                                Orientation="Horizontal"
                                                VerticalAlignment="Center">

                                        <telerikControls:RadSlider Width="200" 
                                                                    Value="{Binding Zoom, Mode=TwoWay, Converter={StaticResource ScaleFactorConverter}}" 
                                                                    Minimum="-100" 
                                                                    SmallChange="10"                                                                   
                                                                    Maximum="100" 
                                                                    VerticalAlignment="Center"
                                                                    HandlesVisibility="Visible">
                                            <ToolTipService.ToolTip>
                                                <ToolTip Content="{Binding TextResources.ZoomToolTip}"/>
                                            </ToolTipService.ToolTip>
                                        </telerikControls:RadSlider>

                                        <telerikInput:RadComboBox Margin="10 0 3 0" Width="70"
                                                                  HorizontalAlignment="Center" VerticalAlignment="Center" 
                                                                  IsEditable="True" IsTextSearchEnabled="False"
                                                                  MaxDropDownHeight="300"
                                                                  ItemsSource="{Binding AvailableZoomValues}"
                                                                  Text="{Binding ZoomText, Mode=TwoWay}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip Content="{Binding TextResources.ZoomToolTip}"/>
                                            </ToolTipService.ToolTip>
                                        </telerikInput:RadComboBox>
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
