    <SolidColorBrush x:Key="ScrollBarThumbGripStroke_Normal" Color="#B2FFFFFF"/>
    <SolidColorBrush x:Key="ScrollBarThumbGripFill_MouseOver" Color="#FFB4BFC9"/>
    <SolidColorBrush x:Key="ScrollBarThumbGripStroke_MouseOver" Color="#B2FFFFFF"/>
    <SolidColorBrush x:Key="ScrollBarThumbGripFill_Disabled" Color="#FFA5A5A5"/>
    <SolidColorBrush x:Key="ScrollBarThumbGripStroke_Disabled" Color="#B2FFFFFF"/>
    <SolidColorBrush x:Key="ScrollBarInnerBorder_Normal" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ScrollBarOuterBorder_Normal" Color="#FFB1B1B1"/>
    <LinearGradientBrush x:Key="ScrollBarBackgroundVertical_Normal" EndPoint="0.087,0.529" StartPoint="0.925,0.529">
