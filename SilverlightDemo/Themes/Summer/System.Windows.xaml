        <GradientStop Color="#FFDFE1E2" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ScrollBarInnerBorderVertical_Normal" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ScrollBarOuterBorderVertical_Normal" Color="#FFB1B1B1"/>
