        <GradientStop Color="#FFFAFAFA" Offset="0.345"/>
        <GradientStop Color="#FFDAE1E4" Offset="0.035"/>
        <GradientStop Color="#FF84ADC5" Offset="0.034"/>
    </LinearGradientBrush>
    <Thickness x:Key="ScrollBarDecreaseButtonMarginHorizontal">0 1</Thickness>
    <Thickness x:Key="ScrollBarIncreaseButtonMarginHorizontal">0 1</Thickness>
    <Thickness x:Key="ScrollBarDecreaseButtonMarginVertical">1 0</Thickness>
    <Thickness x:Key="ScrollBarIncreaseButtonMarginVertical">1 0</Thickness>
